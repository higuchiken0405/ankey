<div class="wrapper-sessions-new">
  <div class="container text-center">
  <h1>ログイン</h1>
    <div class="row">
      <%= form_for :session, url: login_path do |f| %>
        <div class="form-group">
          <%= f.label :"メールアドレス" %><br>
          <%= f.email_field :email %>
        </div>
        <div class="form-group">
          <%= f.label :"パスワード" %><br>
          <%= f.password_field :password %>
        </div>

        <%= f.submit "ログイン", class: "btn btn-info" %>

      <% end %>
    </div>

  </div>
</div>
