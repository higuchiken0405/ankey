<div class="container text-center" id="pages-login-form-modal">
  <div class="container-pages-login">
    <div class="modal-form-pages-login">
      <div class="text-right"><i class="close-modal">Ã—</i></div>
      <h1>Log in</h1>
      <%= form_for :session, url: login_path do |f| %>
        <div class="form-group">
          <%= f.label :"email", class: "form-title" %><br>
          <%= f.text_field :email, class: "login-pages-form" %>
        </div>
        <div class="form-group">
          <%= f.label :"password", class: 'form-title' %><br>
          <%= f.password_field :password, class: "login-pages-form" %>
        </div>
        <%= f.submit "Log in", class: "btn login-btn" %>
      <% end %>
    </div>
  </div>
</div>
